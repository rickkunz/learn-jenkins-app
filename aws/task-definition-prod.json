{
  "requiresCompatibilities": [
      "FARGATE"
  ],
  "family": "LearnJenkinsApp-TaskDefinition-Prod",
  "containerDefinitions": [
      {
          "name": "learnjenkinsapp",
          "image": "314146310905.dkr.ecr.us-east-1.amazonaws.com/learnjenkinsapp:1.0.76",
          "portMappings": [{
              "name": "nginx-80-tip",
              "containerPort": 80,
              "hostPort": 80,
              "protocol": "tcp",
              "appProtocol": "http"
          }],
          "essential": true
      }
  ],
  "volumes": [],
  "networkMode": "awsvpc",
  "memory": "512",
  "cpu": "256",
  "executionRoleArn": "arn:aws:iam::314146310905:role/ecsTaskExecutionRole"
}